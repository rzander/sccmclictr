                                                <StackPanel Orientation="Horizontal" >
                                                    <Image Source="/SCCMCliCtrWPF;component/Images/msi.ico" Stretch="Uniform" Width="16" Height="16"/>
                                                    <TextBlock Text="Installed Software" Margin="3,0,0,0"></TextBlock>
                                                </StackPanel>
                                            </TreeViewItem.Header>
                                        </TreeViewItem>
                                    </TreeViewItem>
                                </TreeView>
                            </StackPanel>
                        </s:NavigationPaneItem>
                        <s:NavigationPaneItem Name="SWDistPane" Header="Software Distribution" Image="/SCCMCliCtrWPF;component/Images/Import%20File.ico" ImageSmall="/SCCMCliCtrWPF;component/Images/Import%20File.ico">
                            <StackPanel VerticalAlignment="Stretch">
                                <TreeView VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Height="200" Name="tvSWDist" Background="#00000000" BorderBrush="#00000000" Foreground="#00000000" MouseLeftButtonDown="treeView1_MouseLeftButtonDown" Loaded="tvSWDist_Loaded">
                                    <TreeViewItem Name="tviSWDistOverview"  Header="Overview" FontSize="14" Margin="0,5,0,0" IsSelected="True" OverridesDefaultStyle="False" IsExpanded="True" Selected="TreeViewItem_Selected">
